<?xml version="1.0" encoding="UTF-8"?><wsdl:definitions name="order"	targetNamespace="http://www.realdolmen.com/orders" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"	xmlns:tns="http://www.example.org/order/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"	xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns:rd="http://www.realdolmen.com/orders">	<wsdl:types>		<xsd:schema>			<xsd:element name="orderBook">				<xsd:complexType>					<xsd:sequence>						<xsd:element name="bookId" type="xsd:integer" minOccurs="1" maxOccurs="1"/>					</xsd:sequence>				</xsd:complexType>			</xsd:element>			<xsd:element name="order">				<xsd:complexType>					<xsd:sequence>						<xsd:element name="id" type="xsd:integer" minOccurs="1" maxOccurs="1"/>						<xsd:element name="status" type="xsd:integer" minOccurs="1" maxOccurs="1"/>						<xsd:element name="dateCreated" type="xsd:string" minOccurs="1" maxOccurs="1"/>						<xsd:element name="userId" type="xsd:integer" minOccurs="1" maxOccurs="1"/>					</xsd:sequence>				</xsd:complexType>			</xsd:element>		</xsd:schema>	</wsdl:types>	<wsdl:message name="orderRequestMessage">		<wsdl:part name="myInput" element="orderBook" />	</wsdl:message>	<wsdl:message name="orderResponseMessage">		<wsdl:part name="myOutput" element="order" />	</wsdl:message>	<wsdl:portType name="orderPortType">		<wsdl:operation name="orderBook">			<wsdl:input message="rd:orderRequestMessage"></wsdl:input>			<wsdl:output message="rd:orderResponseMessage"></wsdl:output>		</wsdl:operation>	</wsdl:portType>	<wsdl:binding name="orderBinding" type="rd:orderPortType">		<soap12:binding transport="http://schemas.xmlsoap.org/soap/http"			style="document" />		<wsdl:operation name="orderBook">			<soap12:operation soapAction="" />			<wsdl:input>				<soap12:body use="literal" />			</wsdl:input>			<wsdl:output>				<soap12:body use="literal" />			</wsdl:output>		</wsdl:operation>	</wsdl:binding>	<wsdl:service name="orderService">		<wsdl:port name="orderPort" binding="rd:orderBinding">			<soap12:address location="http://localhost:8080/whatever" />		</wsdl:port>	</wsdl:service></wsdl:definitions>